{
  "expo": {
    "name": "Tennis Shop",
    "slug": "tennisShop",
    "privacy": "public",
    "description": "A fullstack MERN React Native Ecommerce mobile app for online shopping for tennis equipment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/tennis.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/tennis.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "tennis-favicon": "./assets/tennis.png"
    }
  }
}
